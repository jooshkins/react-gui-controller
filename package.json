{
	"name": "react-gui-controller",
	"version": "1.0.3",
	"description": "A graphical user interface for changing states in react. Inspired from Google's popular dat.GUI controller library. This library provides additional functionalities such as Ease curve editor, Enhanced hue selector, Draggable placement and Stylable component's. For styling this library uses Zeit styled-jsx.",
	"main": "build/index.js",
	"scripts": {
		"lib": "babel src/lib -d build --copy-files",
		"lib:watch": "babel src/lib -w -d build --copy-files",
		"start": "webpack-dev-server --mode development --open --hot",
		"build": "webpack --mode production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pizza3/react-gui-controller.git"
	},
	"author": "Yugam Dhuriya",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pizza3/react-gui-controller/issues"
	},
	"homepage": "https://github.com/pizza3/react-gui-controller#readme",
	"dependencies": {
		"react-router-dom": "^4.3.1",
		"styled-jsx": "^2.2.6"
	},
	"peerDependencies": {
		"react": "^16.4.0",
		"react-dom": "^16.4.0"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^8.2.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"eslint": "^4.19.1",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.9.1",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"prop-types": "^15.6.2",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "^4.12.0",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	}
}
